
package com.nighthawk.csa.Minilab.Jacob.SortsPOJO;

public class Insertion extends Base {
    public Insertion() {
    }

    public Name[] sortArr(Name[] arr) {
        int n = arr.length;

        for(int i = 1; i < n; ++i) {
            Name key = arr[i];

            int j;
            for(j = i - 1; j >= 0 && arr[j].compareTo(key) > 0; --j) {
                arr[j + 1] = arr[j];
            }

            arr[j + 1] = key;
        }

        return arr;
    }
}
