package com.nighthawk.csa.Minilab.Jacob.SortInt;

public class Insertion extends Base {
    public Insertion() {
    }

    public int[] sortArr(int[] arr) {
        int n = arr.length;

        for(int i = 1; i < n; ++i) {
            int key = arr[i];

            int j;
            for(j = i - 1; j >= 0 && arr[j] > key; --j) {
                arr[j + 1] = arr[j];
            }

            arr[j + 1] = key;
        }

        return arr;
    }
}
